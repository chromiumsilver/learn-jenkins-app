{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "AWS_ECS_TASK_DEF",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "AWS_ACCOUNT_ID.dkr.ecr.AWS_DEFAULT_REGION.amazonaws.com/APP_NAME:LATEST_TD_REVISION",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::AWS_ACCOUNT_ID:role/ecsTaskExecutionRole"
}
